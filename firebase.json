{
  "hosting": {
    "public": "main",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "function": "app"
      }
    ]
  },
   "functions": {
     "source": "./functions",
     "runtime": "nodejs18"
  }
}
